#include <iostream> 
using namespace std;
int main() {
    std:: vector<double> prices = {1,2,3,4,5};
    
    prices.push_back(10);
    prices.push_back(50);
    prices.push_back(11);
    
    double total = prices[0] + prices[1] + prices[2];
    
    cout << "total " << total << endl;

    return 0;
}




